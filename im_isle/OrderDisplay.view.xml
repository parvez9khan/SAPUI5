<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc"
	xmlns:f="sap.ui.layout.form" xmlns:l="sap.ui.layout" xmlns="sap.m"
		controllerName="im_isle.Home" xmlns:html="http://www.w3.org/1999/xhtml">
<Page>
	<headerContent>
			<OverflowToolbar>
				<Image
					src="data:image/png;base64,"
					press="homepage" class="mylogo"/>
					<Image	src="./image/Hermes.png" class="myhermes"/>
<!-- 					<Image	src="./image/SAPfiorilogo.jpg" class="myfiorilogo"/> -->
<!-- 					<Label text="Hermes" level="12" class="myhermes"/> -->
				<SearchField width="auto" placeholder="Search" id="__field01" class="mytoolbaritem"></SearchField>
				<ToolbarSpacer/>
				<MenuButton id="_menubtn1" text="Hello Mr.Smith" icon="sap-icon://customer" class="mytoolbaritem">
					<menu>
						<Menu itemSelected="onMenuAction">
							<items>
								<MenuItem text="My Orders" icon="sap-icon://my-sales-order"/>
								<MenuItem text="My Address" icon="sap-icon://address-book"/>
								<MenuItem text="Not Smith? Sign Out" icon="sap-icon://log"/>
							</items>
						</Menu>
					</menu>
				</MenuButton>
				<Button id="_homecart1" text="Cart(4)" icon="sap-icon://cart" press="cart" class="mytoolbaritem"/>
			</OverflowToolbar>
		</headerContent>
		<content>
			<Panel>
				<content>
					<VBox class="sapUiSmallMargin">
						<f:SimpleForm id="SimpleFormChange480" minWidth="1024"
							maxContainerCols="2" editable="true" layout="ResponsiveGridLayout"
							labelSpanL="4" labelSpanM="4" emptySpanL="0" emptySpanM="0"
							columnsL="2" columnsM="2"  class="editableForm">
							<f:content>
								<core:Title text="Order Details" />
								<Label text="Order ID" />
								<Input id="_order" value="7000002006" editable="false"/>
<!-- 								<Input id="companyInput" -->
<!-- 		value="{/company/name}" -->
<!-- 		tooltip="The name of the company is '{/company/name}'}"/> -->
<!-- 								<Input  value="{/Order}" editable="false"/> -->
								<Label text="Invoice No." />
								<Input value="9905222158" editable = "false"/>
								<Link text="View Invoice" href="./image/OMT_1.pdf" target="_blank" emphasized="true" press="download">
									<layoutData>
										<l:GridData span="L5 M5 S4" />
									</layoutData>
								</Link>
                    		   
<!-- 								<html:a href="./image/OMT_1.pdf" download="Invoice">Download Invoice</html:a> -->
								<Label text="Customer PO" />
								<Input value="9000123987" editable="false"/>
								<Label text="Currency" />
								<Input value="US Dollar" editable="false"/>
								<Label text="Price List" />
								<Input value="" editable="false"/>
								<Label text="Price Locked" />
								<Select width="25%" >
									<items >
										<core:Item text="Yes" editable= "false"/>
										<core:Item text="No" enabled = "false"/>
									</items>
								</Select>

								<core:Title text="Ship to Address" />
								<Label text="Name" />
								<Input value="Amazon US" editable="false"/>
								<Label text="Street/No." />
								<Input value="Terry Ave" editable="false">
								</Input>
								<Input value="410" editable="false">
									<layoutData>
										<l:GridData span="L3 M3 S4" />
									</layoutData>
								</Input>
								<Label text="ZIP Code/City" editable="false"/>
								<Input value="588150" editable="false">
									<layoutData>
										<l:GridData span="L3 M3 S4" />
									</layoutData>
								</Input>
								<Input value="New York" editable="false"/>
								<Label text="Country" />
								<Select width="30%">
									<items >
										<core:Item text="USA" editable = "false"/>
										<core:Item text="Germany" enabled = "false"/>
										<core:Item text="England" enabled = "false"/>
									</items>
								</Select>
								<core:Title text="Sales Information" />
								<Label text="Opportunity" />
								<Input value="Sales" editable = "false"/>
								<Label text="Quote" />
								<Input value="Available" editable = "false"/>
								<Label text="Potential Customer" />
								<Input value="Yes" editable = "false"/>
								<Label text="End User" />
								<Input value="John K" editable = "false"/>
								<core:Title text="ERP Information" />
								<Label text="ERP Order ID" />
								<Input value="9000123987" editable = "false"/>
								
								<Label text="Order Message" />
								<Input value="Successful" editable = "false"/>
								<Label text="Invoice System Date" />
								<DatePicker id="DP1" placeholder="Enter Date ..." value="11/15/16" editable = "false"
									change="handleChange" class="sapUiSmallMarginBottom" />
							</f:content>
						</f:SimpleForm>
					</VBox>
				</content>
			</Panel>
						<Panel headerText="Shopping Cart">
				<content>
					<Table id="idProductsTable" inset="false" items="{ordercart>/rootNode}">
						<!-- <headerToolbar> -->
						<!-- <Toolbar> -->
						<!-- <ToolbarSpacer/> -->
						<!-- <Button text="Remove" type="Reject"/> -->
						<!-- </Toolbar> -->
						<!-- </headerToolbar> -->
						<columns>

							<Column minScreenWidth="Desktop" demandPopin="true">
								<Text text=" " />
							</Column>
							<Column width="250px">
								<Text text="Product" />
							</Column>
							<Column minScreenWidth="Desktop" demandPopin="true" width="70px">
								<Text text="Properties" />
							</Column>
							<Column minScreenWidth="Desktop" demandPopin="true" hAlign="Right"
								width="50px">
								<Text text="Unit" />
							</Column>
							<Column minScreenWidth="Desktop" demandPopin="true" hAlign="Center"
								width="70px">
								<Text text="Price/Unit" />
							</Column>
							<Column minScreenWidth="Phone" demandPopin="true"  hAlign="Center" width="70px">
								<Text text="Quantity" />
							</Column>
							<Column minScreenWidth="Desktop" demandPopin="true"  hAlign="Center" width="70px">
								<Text text="Discount" />
							</Column>
							<Column minScreenWidth="Desktop" demandPopin="true"  hAlign="Center" width="150px">
								<Text text="Extended Amount" />
							</Column>
							<Column minScreenWidth="Desktop" demandPopin="true"  hAlign="Center" width="80px">
								<Text text="Suggestions" />
							</Column>
							<Column minScreenWidth="Desktop" demandPopin="true"  hAlign="Center" >
							<Text text="Status" />
							</Column>
						</columns>
						<items>
							<ColumnListItem>
								<cells>
									<Image src="{ordercart>URL}" />
									<ObjectIdentifier title="{ordercart>Name}" titleActive="true"
										text="{ordercart>ProductId}"/>
										<!-- <ObjectNumber -->
										<!-- number="{cartitem>Price}" -->
										<!-- unit="$" -->
										<!-- state="Success" /> -->
									<!--</ObjectIdentifier>-->

									<Text text="{ordercart>Properties}" />
									<Text text="{ordercart>Unit}" />
									<Text text="${ordercart>Price}" />
									<Text text="{ordercart>Quantity}" />
									<Text text="{ordercart>Discount}" />

									<Text text="{ordercart>ExtendedAmount}" />
									<Text text="{ordercart>Suggestions}" />
										<ObjectStatus class="sapUiSmallMarginBottom"
											text="{ordercart>Delivery}"
											state="{ordercart>DStatus}"/>
<!-- 											"{=${ordercart>Price} > 300 ? 'Success':'Error' } -->
<!-- 									<Button text="Remove" icon="sap-icon://decline" type="Reject" /> -->
								</cells>
							</ColumnListItem>
							
						</items>

					</Table>
				</content>
			</Panel>
		
		</content>
		<footer>
			<Bar id="_footerBar">
				<contentRight>
					<!--<Button text="Add to Cart" icon="sap-icon://cart" type="Accept" press="addtocart"></Button>-->
					<Label id="_total" text="Total:$932.00" design="Bold"/>
					<Button icon="sap-icon://write-new" type="Accept" text="Write Review" press="writereview"></Button>
					<Button icon="sap-icon://home" type="Emphasized" text="home" press="homepage"></Button>
				</contentRight>
			</Bar>
		</footer>
	</Page>
</core:View>